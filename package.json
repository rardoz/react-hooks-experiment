{
  "name": "react-hooks-experiment",
  "version": "1.0.0",
  "description": "An experiment with react hooks",
  "main": "app/client.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "start:dev": "npm run generate-imported-component && parcel app/index.html",
    "build": "rimraf dist && npm run generate-imported-component && npm run build-client && npm run build-server",
    "build-client": "BABEL_ENV=client parcel build app/index.html -d dist/client --public-url /dist",
    "build-server": "BABEL_ENV=server parcel build server/index.js -d dist/server --public-url /dist --target=node6",
    "start:server": "npm run build && node dist/server",
    "generate-imported-component": "imported-components app app/imported.js",
    "lint": "prettier --single-quote  --no-semi --write \"app/**/*.{js,css}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add ."
    }
  },
  "author": "Robert Greene",
  "license": "ECL-2.0",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.1.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "husky": "^1.3.1",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.16.4",
    "react-hooks-testing-library": "^0.3.4",
    "react-hot-loader": "^4.8.0",
    "rimraf": "^2.6.3",
    "sass": "^1.17.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "hella-bomb-react-context": "^1.0.0",
    "hookrouter": "^1.0.3-beta",
    "jest": "^24.1.0",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-imported-component": "^5.5.3",
    "universal-location-middleware": "^1.0.16"
  }
}
